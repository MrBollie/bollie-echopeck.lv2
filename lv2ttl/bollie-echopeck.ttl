@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix time: <http://lv2plug.in/ns/ext/time#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://ca9.eu/bollie#me>
    a foaf:Person ;
    foaf:name "Bollie" ;
    foaf:mbox <mailto:bollie@ca9.eu> ;
    foaf:homepage <https://ca9.eu/lv2> .

<https://ca9.eu/lv2/bollie-echopeck>
    a lv2:Plugin, lv2:DelayPlugin, doap:Project;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    doap:maintainer <http://ca9.eu/bollie#me> ;
    lv2:microVersion 0 ; lv2:minorVersion 1 ;
    doap:name "Bollie Echopeck";
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "input_control" ;
        lv2:name "INPUT CONTROL";
        lv2:default 5 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "length_of_swell" ;
        lv2:name "LENGTH OF SWELL";
        lv2:default 5 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "volume" ;
        lv2:name "VOLUME";
        lv2:default 5 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "bass_treble" ;
        lv2:name "BASS TREBLE";
        lv2:default 0 ;
        lv2:minimum -5 ;
        lv2:maximum 5 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "selector" ;
        lv2:name "SELECTOR" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 2 ;
        lv2:portProperty lv2:enumeration, lv2:integer;
        lv2:scalePoint [
            rdf:value 0 ;
            rdfs:label "ECHO" ;
            rdfs:comment "ECHO MODE" ;
        ], [
            rdf:value 1 ;
            rdfs:label "REP" ;
            rdfs:comment "REPEAT MODE" ;
        ], [
            rdf:value 2 ;
            rdfs:label "SWELL" ;
            rdfs:comment "SWELL MODE" ;
        ];
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "switch" ;
        lv2:name "SWITCH" ;
        lv2:default 1 ;
        lv2:minimum 1 ;
        lv2:maximum 12;
        lv2:portProperty lv2:enumeration, lv2:integer;
        lv2:scalePoint 
            [ rdf:value 1 ; rdfs:label "1" ; ],
            [ rdf:value 2 ; rdfs:label "2" ; ],
            [ rdf:value 3 ; rdfs:label "3" ; ],
            [ rdf:value 4 ; rdfs:label "4" ; ],
            [ rdf:value 5 ; rdfs:label "5" ; ],
            [ rdf:value 6 ; rdfs:label "6" ; ],
            [ rdf:value 7 ; rdfs:label "7" ; ],
            [ rdf:value 8 ; rdfs:label "8" ; ],
            [ rdf:value 9 ; rdfs:label "9" ; ],
            [ rdf:value 10 ; rdfs:label "10" ; ],
            [ rdf:value 11 ; rdfs:label "11" ; ],
            [ rdf:value 12 ; rdfs:label "12" ; ];
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 6 ;
        lv2:symbol "in" ;
        lv2:name "In"
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 7 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "level_indicator" ;
        lv2:name "LEVEL INDICATOR" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "trim_dry" ;
        lv2:name "TRIM DRY";
        lv2:default 9 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "trim_head_1" ;
        lv2:name "TRIM HEAD 1";
        lv2:default 6 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "trim_head_2" ;
        lv2:name "TRIM HEAD 2";
        lv2:default 6 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "trim_head_3" ;
        lv2:name "TRIM HEAD 3";
        lv2:default 6 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "trim_head_4" ;
        lv2:name "TRIM HEAD 4";
        lv2:default 6 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 14 ;
        lv2:symbol "trim_length_of_swell" ;
        lv2:name "TRIM LENGTH OF SWELL";
        lv2:default 6 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
    ] ;
    rdfs:comment '''A try to emulate a certain echo effect.
    Enjoy! :-) And feedback is always welcome.''' .
